{
  routes: {
    '/person/:id/*':{
      components: {
        sidebar: {
          items:[
            {
              component: 'Link',
              properties: {
                label: 'Emails',
                to: 'email',
              },
            },
          ]
        }
        ,
        main: [
          {
            path: 'emails',
            label: 'Emails',
            component: 'RecordDisplay',
            properties: {
              table: 'person_email',
              conditions: [{ eql: 'person_id={{person_id}}' }],
              rows: [
                '{{email}}',
              ],
            },
          },
        ],
      },
    },
  },
}
