{
  menu: {
    "data-menu":{
          title: 'Data',
          type: 'collapse',
          children: {
            job:{
              title: 'Jobs',
              icon:'message',
              url: '/job',
            }
          },
    },
  },
  routes: {
    '/job':{
      layout: 'full_width',
      components: {
        main: [
          {
            component: 'RecordList',
            properties: {
              table: 'job',
              columns: [
                {
                  title: 'Id',
                  dataIndex: 'id',
                  sorter: true,
                  width: '20%',
                },
                {
                  title: 'Created',
                  dataIndex: 'created_at',
                  sorter: true,
                  width: '40%',
                },
                {
                  title: 'Name',
                  dataIndex: 'name',
                  sorter: true,
                  render: '{{label}}',
                  width: '40%',
                }
              ],
              onRow: {
                onClick: {
                  action: 'navigate',
                  url: '/job/{{record.id}}',
                },
              },
            },
          },
        ],
      },
    },
    '/job/:id/*':{
      layout: 'sidebar',
      components: {
        sidebar: [
          {
            component: 'RecordDisplay',
            properties: {
              table: 'message',
              id: '{{parameters.id}}',
              components: [
                '{{label}}',
              ],
            },
          }
        ],
        main: [
          {
            // 'path':'overview', //no path is the default
            component: 'RecordDisplay',
            properties: {
              table: 'message',
              id: '{{parameters.id}}',
              components: [
                '{{label}}',
              ],
            },
          },
          {
            path: 'source_codes',
            label: 'Source Codes',
            component: 'RecordDisplay',
            properties: {
              table: 'message',
              id: '{{parameters.id}}',
              extensions: {
                source_codes:{
                  table:'source_code'
                }
              },
              components: [
                '{{label}}',
              ],
            },
          },
        ],
      },
    },
  },
}
